<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sinosoft.core.db.model">
	<class
		name="BDCompany"
		table="BD_COMPANY"
	>

		<meta attribute="sync-DAO">true</meta>

		<id
			name="Dbid"
			type="integer"
			column="DBID"
		>
			<!-- <generator class="sequence">
			<param name = "sequence">SEQ_BD_COMPANY</param>
			</generator> -->
			<generator class="native"/>
		</id>

		<property
			name="CompanyCode"
			column="COMPANY_CODE"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="CompanyName"
			column="COMPANY_NAME"
			type="string"
			not-null="true"
			length="120"
		/>
		<property
			name="CompanyShortName"
			column="COMPANY_SHORT_NAME"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="CompanyEnglishName"
			column="COMPANY_ENGLISH_NAME"
			type="string"
			not-null="false"
			length="120"
		/>
		<property
			name="Address"
			column="ADDRESS"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="ZipCode"
			column="ZIP_CODE"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="Phone"
			column="PHONE"
			type="string"
			not-null="false"
			length="18"
		/>
		<property
			name="Fax"
			column="FAX"
			type="string"
			not-null="false"
			length="18"
		/>
		<property
			name="Email"
			column="EMAIL"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="WebAddress"
			column="WEB_ADDRESS"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="CompanyGrade"
			column="COMPANY_GRADE"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="ComanyAreaType"
			column="COMANY_AREA_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="UpCompanyCode"
			column="UP_COMPANY_CODE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="DbOperator"
			column="DB_OPERATOR"
			type="string"
			not-null="true"
			length="120"
		/>
		<property
			name="InsertTime"
			column="INSERT_TIME"
			type="date"
			not-null="true"
			length="11"
		/>
		<property
			name="LastModifyTime"
			column="LAST_MODIFY_TIME"
			type="date"
			not-null="true"
			length="11"
		/>


	</class>	
</hibernate-mapping>