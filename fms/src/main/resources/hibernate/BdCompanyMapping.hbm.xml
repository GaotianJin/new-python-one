<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sinosoft.core.db.model">
	<class
		name="BDCompanyMapping"
		table="BD_COMPANY_MAPPING"
	>

		<meta attribute="sync-DAO">true</meta>

		<id
			name="Dbid"
			type="integer"
			column="DBID"
		>
			<!-- <generator class="sequence">
			<param name = "sequence">SEQ_BD_COMPANY_MAPPING</param>
			</generator> -->
			<generator class="native"/>
		</id>

		<property
			name="CompanyCode"
			column="COMPANY_CODE"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="Region"
			column="REGION"
			type="string"
			not-null="true"
			length="5"
		/>
		<property
			name="CompanyShortName"
			column="COMPANY_SHORT_NAME"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="CompanyPy2Char"
			column="COMPANY_PY_2_CHAR"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="CompanyPy6Char"
			column="COMPANY_PY_6_CHAR"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="BoSystem"
			column="BO_SYSTEM"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="RlsSystem"
			column="RLS_SYSTEM"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="PolicyPrefix"
			column="POLICY_PREFIX"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="CallCenterSystem"
			column="CALL_CENTER_SYSTEM"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="CityCodeRcms"
			column="CITY_CODE_RCMS"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="UspCode"
			column="USP_CODE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Province"
			column="PROVINCE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Status"
			column="STATUS"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="IcbcBankCode"
			column="ICBC_BANK_CODE"
			type="string"
			not-null="false"
			length="100"
		/>


	</class>	
</hibernate-mapping>