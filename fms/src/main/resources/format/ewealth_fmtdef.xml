				<requestCode>${requestCode}</requestCode>
			</object>
			<object tag="eWealthContent" data="eWealthContent">
				<orderInfoList>
					<loop tag="orderInfo" data="WealthPdInfoList">
						<object tag="orderInfo" data="?">
							<tradeInfoId>${tradeInfoId}</tradeInfoId>
							<name>${custName}</name>
							<productName>${productName}</productName>
							<mechanism>${mechanism}</mechanism>
							<totalAmount>${totalAmount}</totalAmount>
							<investmentDate>${investmentDate}</investmentDate>
							<yield>${yield}</yield>
							<establishDate>${establishDate}</establishDate>
						</object>
					</loop>
				</orderInfoList>
			</object>
		</object>
	</fmtdef>
	
	
