<?xml version="1.0" encoding="UTF-8"?>
<pattern>
	<fmtdef id="eWealthGetProductInfo" object="object" loop="loop">
		<object tag="eWealthXml" data="reqDataMap">
			<object tag="eWealthHeader" data="headData">
				<businessCode>PRO01</businessCode>
				<systemCode>LC01</systemCode>
				<password></password>
				<resultCode></resultCode>
				<resultDesc></resultDesc>
				<signMsg>${signMsg}</signMsg>
				<requestCode>0</requestCode>
			 </object>
			 <object tag="eWealthContent" data="contentData">
				<productCode>${productCode}</productCode>
				<productName>${productName}</productName>
				<productShortName>${productShortName}</productShortName>
				<agencyName>${agencyName}</agencyName>
				<agencyCode>${agencyCode}</agencyCode>
				<productType>${productType}</productType>
				<productSubType>${productSubType}</productSubType>
				<introduceDate>${introduceDate}</introduceDate>
				<tradeWays>${tradeWays}</tradeWays>
				<buyType>${buyType}</buyType>
				<promotionWays>${promotionWays}</promotionWays>
				<saleChnls>${saleChnls}</saleChnls>
				<payWays>${payWays}</payWays>
				<isShow>${isShow}</isShow>
				<isShowFirst>${isShowFirst}</isShowFirst>
				<internetProductType>${internetProductType}</internetProductType>
				<lvchengFlag>${lvchengFlag}</lvchengFlag>
				<hotSaleFlag>${hotSaleFlag}</hotSaleFlag>
				<loop tag="wealthDetail" data="wealthContentList">
				  <object tag="wealthDetail" data="?">
					<saleSumAmount>${saleSumAmount}</saleSumAmount>
					<minTradeAmount>${minTradeAmount}</minTradeAmount>
					<maxTradeAmount>${maxTradeAmount}</maxTradeAmount>
					<investIncreaseMoney>${investIncreaseMoney}</investIncreaseMoney>
					<investDays>${investDays}</investDays>
					<investDaysUnit>${investDaysUnit}</investDaysUnit>
					<closePeriodDesc>${closedPeriodDesc}</closePeriodDesc>
					<beneficalTypes>${beneficalTypes}</beneficalTypes>
					<historyEarnRate>${historyEarnRate}</historyEarnRate>
					<investStartTime>${investStartTime}</investStartTime>
					<investEndTime>${investEndTime}</investEndTime>
					<closeAccountDate>${closeAccountDate}</closeAccountDate>
					<foundDate>${foundDate}</foundDate>
				  </object>
				  <object tag="wealthDetail" data="?">
						<saleSumAmount>${saleSumAmount}</saleSumAmount>
						<minTradeAmount>${minTradeAmount}</minTradeAmount>
						<maxTradeAmount>${maxTradeAmount}</maxTradeAmount>
						<investIncreaseMoney>${investIncreaseMoney}</investIncreaseMoney>
						<investDays>${investDays}</investDays>
						<investDaysUnit>${investDaysUnit}</investDaysUnit>
						<closePeriodDesc>${closedPeriodDesc}</closePeriodDesc>
						<beneficalTypes>${beneficalTypes}</beneficalTypes>
						<investStartTime>${investStartTime}</investStartTime>
						<investEndTime>${investEndTime}</investEndTime>
						<closeAccountDate>${closeAccountDate}</closeAccountDate>
						<foundDate>${foundDate}</foundDate>
					</object>
				</loop>
				<!--保险产品详细信息 -->
				<loop tag="insuranceDetail" data="insuranceContentDataList">
					<object tag="insuranceDetail" data="?">
						<insureYears>${insureYears}</insureYears>
						<insureYearsUnit>${insureYearsUnit}</insureYearsUnit>
						<mainFlag>${mainFlag}</mainFlag>
						<payYears>${payYears}</payYears>
						<minAppAge>${minAppAge}</minAppAge>
						<minAppAgeUnit>${minAppAgeUnit}</minAppAgeUnit>
						<maxAppAge>${maxAppAge}</maxAppAge>
						<maxAppAgeUnit>${maxAppAgeUnit}</maxAppAgeUnit>
						<description>${description}</description>
					</object>
				</loop>
				<inputInfo>
					<loop tag="inputItem" data="inputItemListData">
						<object tag="inputItem" data="?">
							<inputCode>${inputCode}</inputCode>
							<inputScope>${inputScope}</inputScope>
							<isMust>${isMust}</isMust>
							<seq>${seq}</seq>
						</object>
					</loop>
				</inputInfo>
				<showInfo>
					<loop tag="showItem" data="showItemDataList">
						<object tag="showItem" data="?">
							<itemName>${itemName}</itemName>
							<itemValue>${itemValue}</itemValue>
							<showAreaType>${showAreaType}</showAreaType>
							<seq>${seq}</seq>
							<description>${description}</description>
						</object>
					</loop>
				</showInfo>
				<productFeeRateInfo>
					<loop tag="productFeeRateItem" data="productFeeRateItemListData">
						<object tag="productFeeRateItem" data="?">
							<subscriptionFeeUpper>${subscriptionFeeUpper}</subscriptionFeeUpper>
							<subscriptionFeeLower>${subscriptionFeeLower}</subscriptionFeeLower>
							<beneficialType>${beneficalType}</beneficialType>
							<closedPeriod>${closedPeriod}</closedPeriod>
							<closedPeriodUnit>${closedPeriodUnit}</closedPeriodUnit>
							<feeRate>${feeRate}</feeRate>
							<expectedFeeRate>${expectedFeeRate}</expectedFeeRate>
							<addExpectedFeeRate>${addExpectedFeeRate}</addExpectedFeeRate>
						</object>
					</loop>
				</productFeeRateInfo>
				<customerInfo>
					<loop tag="customerItem" data="customerItemListData">
						<object tag="customerItem" data="?">
							<inputCode>${inputCode}</inputCode>
							<inputScope>${inputScope}</inputScope>
							<isMust>${isMust}</isMust>
							<seq>${seq}</seq>
						</object>
					</loop>
				</customerInfo>
				<productInfo>${productInfo}</productInfo>
				<appendixInfo>
					<loop tag="appendixItem" data="appendixItemListData">
						<object tag="appendixItem" data="?">
							<appendixName>${appendixName}</appendixName>
							<appendixType>${appendixType}</appendixType>
							<appendixUrl>${appendixUrl}</appendixUrl>
							<description>${description}</description>
						</object>
					</loop>
				</appendixInfo>
			</object>
		</object>
	</fmtdef>

