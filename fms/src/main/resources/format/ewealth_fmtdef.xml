	<fmtdef id="tradeRiskInfo" object="object" loop="loop">
		<object tag="eWealthXml" data="messageTotalMap">
			<object tag="eWealthHeader" data="eWealthHeader">
				<businessCode>${businessCode}</businessCode>
				<systemCode>${systemCode}</systemCode>
				<password>${password}</password>
				<resultCode>${resultCode}</resultCode>
				<resultDesc>${resultDesc}</resultDesc>
				<signMsg></signMsg>
				<requestCode>${requestCode}</requestCode>
			</object>
			<object tag="eWealthContent" data="eWealthContent">
				<tradeRiskList>
					<loop tag="tradeRiskInfo" data="tradeRiskList">
						<object tag="tradeRiskInfo" data="?">
							<tradeNo>${tradeNo}</tradeNo>
							<customerName>${customerName}</customerName>
							<productName>${productName}</productName>
							<agencyName>${agencyName}</agencyName>
							<amntTotal>${amntTotal}</amntTotal>
							<premium>${premium}</premium>
							<tradeDate>${tradeDate}</tradeDate>
						</object>
					</loop>
				</tradeRiskList>
			</object>
		</object>
	</fmtdef>
	<fmtdef id="getWealthPdInfo" object="object" loop="loop">
		<object tag="eWealthXml" data="messageTotalMap">
			<object tag="eWealthHeader" data="eWealthHeader">
				<businessCode>${businessCode}</businessCode>
				<systemCode>${systemCode}</systemCode>
