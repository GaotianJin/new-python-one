<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sinosoft.core.domain.model.workflow">
	<class
		name="Workflow"
		table="workflow"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Url"
			column="url"
			type="string"
			not-null="false"
			length="150"
		/>


	</class>	
</hibernate-mapping>