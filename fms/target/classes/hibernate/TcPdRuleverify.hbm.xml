<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sinosoft.core.domain.model.rule">
	<class
		name="TcPdRuleverify"
		table="TC_PD_RULEVERIFY"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="integer"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="RiskId"
			column="RISK_ID"
			type="integer"
			not-null="true"
			length="20"
		/>
		<property
			name="RuleId"
			column="RULE_ID"
			type="integer"
			not-null="true"
			length="20"
		/>
		<property
			name="VerifyType"
			column="VERIFY_TYPE"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="VerifyGrade"
			column="VERIFY_GRADE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="VerifyOrder"
			column="VERIFY_ORDER"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="PassFlag"
			column="PASS_FLAG"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Remark"
			column="REMARK"
			type="string"
			not-null="false"
		/>


	</class>	
</hibernate-mapping>