<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sinosoft.core.domain.model.quartz">
	<class
		name="Scheduleinfo"
		table="scheduleinfo"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="string"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="ScheduleName"
			column="ScheduleName"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="CronExpression"
			column="CronExpression"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="TargetObject"
			column="TargetObject"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="TargetMethod"
			column="TargetMethod"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="Concurrent"
			column="Concurrent"
			type="string"
			not-null="true"
			length="50"
		/>


	</class>	
</hibernate-mapping>