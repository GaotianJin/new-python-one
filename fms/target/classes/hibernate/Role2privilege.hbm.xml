<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sinosoft.core.domain.model.user">
	<class
		name="Role2privilege"
		table="role2privilege"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="native"/>
		</id>

		<many-to-one
			name="Role"
			column="role_id"
			class="Role"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Privilege"
			column="privilege_id"
			class="Privilege"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>